{
  "name": "node-mysql-helper",
  "version": "1.0.0",
  "description": "A simple MySQL helper for Node.js",
  "main": "dist/index.js",
  "scripts": {
    "main": "dist/index.js",
    "start": "tsx src/index.ts",
    "dev": "tsx watch test/index.ts",
    "dev:node14": "nodemon test/index.js",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "loglevel": "^1.9.1",
    "mysql2": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^22.4.1",
    "nodemon": "^3.1.4",
    "tsc-alias": "^1.8.10",
    "tsx": "4.6.2",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist",
    "jsconfig.json"
  ],
  "packageManager": "yarn@3.2.0"
}
